{
  "version": 2,
  "builds": [
    {
      "src": "src/graphql.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/graphql",
      "dest": "src/graphql.ts",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "Content-Type, Accept, Authorization",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ]
}